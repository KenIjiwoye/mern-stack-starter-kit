    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-promise-router": "^3.0.3",
    "mongoose": "^5.2.17",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
  
