  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "cd client && yarn start",
    "mobile": " cd mobileClient && expo start",
    "client dev": "concurrently \"yarn server\" \" yarn client\"",
    "mobile dev": "concurrently \"yarn server\" \" yarn mobile\"",
    "dev all": "concurrently \"yarn server\" \" yarn client\"\"yarn mobile\""
  },
  "dependencies": {
